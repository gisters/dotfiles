Host bitbucket.org
    HostName        altssh.bitbucket.org
    IdentityFile    ~/.ssh/id_rsa
    Port            443
    User            git

Host github.com
    HostName        ssh.github.com
    IdentityFile    ~/.ssh/id_rsa
    Port            443
    User            git

Compression         yes
Protocol            2
#AddKeysToAgent      yes
UseKeychain         yes
